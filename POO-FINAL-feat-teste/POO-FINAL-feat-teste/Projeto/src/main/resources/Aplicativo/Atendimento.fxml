<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane maxHeight="600.0" maxWidth="500.0" minHeight="600.0" minWidth="500.0" prefHeight="600.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controladores.AtendimentoController">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Label text="Atendimento" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.valignment="TOP" />
      <Label text="Selecione o Evento:" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.valignment="TOP">
         <GridPane.margin>
            <Insets top="30.0" />
         </GridPane.margin>
      </Label>
      <Button fx:id="botaoSeca" mnemonicParsing="false" onAction="#setVisibleSeca" text="Seca" GridPane.columnIndex="2" GridPane.columnSpan="2" GridPane.halignment="LEFT" />
      <Button fx:id="botaoCiclone" mnemonicParsing="false" onAction="#setVisibleCiclone" text="Ciclone" GridPane.columnIndex="1" GridPane.halignment="CENTER">
         <GridPane.margin>
            <Insets left="32.0" />
         </GridPane.margin>
      </Button>
      <Button fx:id="botaoTerremoto" mnemonicParsing="false" onAction="#setVisibleTerremoto" text="Terremoto" GridPane.columnSpan="2" GridPane.halignment="CENTER" />
      <TextField fx:id="textRecebe2" promptText="Magnitude" visible="false" GridPane.columnIndex="1" GridPane.rowIndex="1" GridPane.valignment="TOP" />
      <TextField fx:id="textRecebe" visible="false" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Button mnemonicParsing="false" onAction="#botaoInicialVoltarAtend" text="Voltat" GridPane.valignment="TOP">
         <GridPane.margin>
            <Insets left="15.0" top="15.0" />
         </GridPane.margin></Button>
   </children>
</GridPane>
